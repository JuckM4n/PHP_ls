+        $this->out = &$this->stream;
+    }
+
+    /**
+     * Opens a process for input/output.
+     */
+    private function establishProcessConnection()
+    {
+        $command = $this->params['command'];
+        $descriptorSpec = array(
+            0 => array('pipe', 'r'),
+            1 => array('pipe', 'w'),
+            2 => array('pipe', 'w'),
+            );
