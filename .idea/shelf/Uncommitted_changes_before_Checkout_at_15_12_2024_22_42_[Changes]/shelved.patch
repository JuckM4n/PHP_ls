+<?php
+
+class Swift_ByteStream_FileByteStreamAcceptanceTest extends \PHPUnit\Framework\TestCase
+{
+    private $_testFile;
+
+    protected function setUp()
+    {
+        $this->testFile = sys_get_temp_dir().'/swift-test-file'.__CLASS__;
