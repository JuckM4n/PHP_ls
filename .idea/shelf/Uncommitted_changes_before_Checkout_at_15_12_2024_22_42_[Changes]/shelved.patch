+        $logger->add("<< 502 That makes no sense\r\n");
+        $logger->add(">> RSET\r\n");
+        $logger->add("<< 250 OK\r\n");
+
+        $this->assertEquals(
+            ">> FOO\r\n".PHP_EOL.
+            "<< 502 That makes no sense\r\n".PHP_EOL.
+            ">> RSET\r\n".PHP_EOL.
+            "<< 250 OK\r\n",
