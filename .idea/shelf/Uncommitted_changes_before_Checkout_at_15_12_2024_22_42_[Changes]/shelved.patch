+        $file->setFilename('foo.pdf');
+        $this->assertEquals(
+            'Content-Type: application/pdf; name=foo.pdf'."\r\n".
+            'Content-Transfer-Encoding: base64'."\r\n".
+            'Content-ID: <'.$id.'>'."\r\n".
+            'Content-Disposition: inline; filename=foo.pdf'."\r\n",
+            $file->toString()
