+    {
+        if ($this->writeBuffer !== '') {
+            $this->doWrite($this->writeBuffer);
+        }
+        $this->flush();
+
+        foreach ($this->mirrors as $stream) {
+            $stream->flushBuffers();
+        }
+    }
