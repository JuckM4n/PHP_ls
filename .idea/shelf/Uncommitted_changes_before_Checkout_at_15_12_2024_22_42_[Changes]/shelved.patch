+
+        $serverStarted = false;
+        for ($i = 0; $i < 5; ++$i) {
+            $this->randomHighPort = rand(50000, 65000);
+            $this->server = stream_socket_server('tcp://127.0.0.1:'.$this->randomHighPort);
+            if ($this->server) {
+                $serverStarted = true;
+            }
